@{
    ViewData["Title"] = "Home Page";
}

<a ></a>

<table class="table">

    <thead>
        <tr>
            <th>Id</th>
            <th>Nombre</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in ViewBag.Datos)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Nombre</td>
            </tr>
        }
    </tbody>

</table>